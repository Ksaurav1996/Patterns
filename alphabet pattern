
/* Printing a star pattern
      ABC
      ABC
      ABC   */


#include <stdio.h>
int main(){
    int i,j,n;
    printf("Enter a number n:-");
    scanf("%d", &n);
    for(i=1; i<=n; i++){
        int a=1;
        for(j=1; j<=n; j++){
            int d= a+64;
            int ch= (char)d;
        printf("%c ", ch);
        a++;
        }
        printf("\n");
    }
    return 0;
}
