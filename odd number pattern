
/* Printing a star pattern
      1
      13
      135   */


#include <stdio.h>
int main(){
    int i,j,n;
    printf("Enter a number n:-");
    scanf("%d", &n);
    for(i=1; i<=n; i++){
        int a=1;
        for(j=1; j<=i; j++)
        printf("%d", j);
        a=a+2;
        printf("\n");
    }
    return 0;
}
